<?xml version="1.0" encoding="UTF-8"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">
    
    <RotationSection label="PPS_Transformations.xml">
        <Rotation name="RP_90_z_270_y_rot" phiX="90*deg" thetaX="0*deg" phiY="180*deg" thetaY="90*deg"
            phiZ="90*deg" thetaZ="270*deg"/>
        <Rotation name="RP_90_x_90_y_rot" phiX="270*deg" thetaX="90*deg" phiY="0*deg" thetaY="180*deg"
            phiZ="0*deg" thetaZ="90*deg"/>
        <Rotation name="RP_90_z_90_y_rot" phiX="90*deg" thetaX="90*deg" phiY="0*deg" thetaY="0*deg"
            phiZ="0*deg" thetaZ="90*deg"/>
        <Rotation name="RP_y_90_rot" phiX="0*deg" thetaX="(90+90)*deg" phiY="90*deg" thetaY="90*deg" phiZ="0*deg" thetaZ="90*deg"/>
    </RotationSection>
    
</DDDefinition>
