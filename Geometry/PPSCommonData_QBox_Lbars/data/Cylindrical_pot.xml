<?xml version="1.0" encoding="UTF-8"?>
<DDDefinition xmlns="http://www.cern.ch/mms/DDL"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.cern.ch/mms/DDL ../DDLSchema.xsd">
    <ConstantsSection label="Cylindrical_pot.xml">
        <Constant name="surface_margin" value="3.5*mm"/>
        <Constant name="outer_diameter_lower_part" value="145*mm"/>
        <Constant name="wall_thickness_lower_part" value="3*mm"/>
        <Constant name="height_lower_part" value="53.5*mm"/>
        <Constant name="outer_diameter_upper_part" value="139*mm"/>
        <Constant name="wall_thickness_upper_part" value="1.5*mm"/>
        <Constant name="height_upper_part" value="85*mm"/>
        <Constant name="shift_upper_lower_part" value="8*mm"/>
        <Constant name="outer_diameter_ferrite" value="145*mm"/>
        <Constant name="thickness_ferrite" value="3*mm"/>
        <Constant name="height_ferrite" value="30*mm"/>
        <Constant name="gap_between_ferrite_lower_part" value="2*mm"/>
        <Constant name="wall_thickness_bottom_wall" value="2*mm"/>
        <Constant name="window_width" value="20*mm"/>
        <Constant name="window_height" value="22*mm"/>
        <Constant name="window_thickness_front" value="0.3*mm"/>
        <Constant name="window_thickness_bottom" value="0.3*mm"/>
        <Constant name="total_height" value="138*mm"/>
        <Constant name="flange_thickness" value="24.5*mm"/>
        <Constant name="distance_flange_upper_part" value="7.5*mm"/>
        <Constant name="ferrite_upper_part_distance" value="[flange_thickness]-[distance_flange_upper_part]"/>
        <Constant name="height" value="[height_lower_part]+[height_upper_part]-[shift_upper_lower_part]"/>
        <Constant name="Cylinder_beam_distance" value="3*mm"/>
    </ConstantsSection>
</DDDefinition>

